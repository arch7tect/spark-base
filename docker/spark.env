#spark.driver.extraJavaOptions
CFG_SPARK_DRIVER_EXTRA__JAVA__OPTIONS=""

#spark.executor.spark.executor.extraJavaOptions
CFG_SPARK_EXECUTOR_EXTRA__JAVA__OPTIONS=""

#spark.driver.extraClassPath
CFG_SPARK_DRIVER_EXTRA__CLASS__PATH=/extralib/*

#spark.executor.extraClassPath
CFG_SPARK_EXECUTOR_EXTRA__CLASS__PATH=/extralib/*

#spark.hadoop.fs.defaultFS
CFG_SPARK_HADOOP_FS_DEFAULT__F__S=file:///data

#spark.hadoop.mapreduce.fileoutputcommitter.algorithm.version
#CFG_SPARK_HADOOP_MAPREDUCE_FILEOUTPUTCOMMITTER_ALGORITHM_VERSION=2

#spark.hadoop.mapreduce.fileoutputcommitter.cleanup-failures.ignored
#CFG_SPARK_HADOOP_MAPREDUCE_FILEOUTPUTCOMMITTER_CLEANUP___FAILURES_IGNORED=true

