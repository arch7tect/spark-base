CFG_SPARK_HISTORY_FS_LOG__DIRECTORY=file:///usr/spark/spark-logs

#spark.eventLog.enabled
CFG_SPARK_EVENT__LOG_ENABLED="true"

#spark.eventLog.dir
CFG_SPARK_EVENT__LOG_DIR=file:///usr/spark/spark-logs

#spark.network.timeout 420000
CFG_SPARK_NETWORK_TIMEOUT=420000

#spark.shuffle.service.enabled
CFG_SPARK_SHUFFLE_SERVICE_ENABLED=true

#spark.sql.hive.metastorePartitionPruning
CFG_SPARK_SQL_HIVE_METASTORE__PARTITION__PRUNING=true

#spark.sql.orc.cache.stripe.details.size
CFG_SPARK_SQL_ORC_CACHE_STRIPE_DETAILS_SIZE=10000

#spark.sql.orc.filterPushdown
CFG_SPARK_SQL_ORC_FILTER__PUSHDOWN=true

#spark.sql.orc.splits.include.file.footer
CFG_SPARK_SQL_ORC_SPLITS_INCLUDE_FILE_FOOTER=true

#spark.sql.parquet.filterPushdown
CFG_SPARK_SQL_PARQUET_FILTER__PUSHDOWN=true

#spark.sql.parquet.mergeSchema
CFG_SPARK_SQL_PARQUET_MERGE__SCHEMA=false

#spark.deploy.defaultCores
CFG_SPARK_DEPLOY_DEFAULT__CORES=4

#spark.dynamicAllocation.cachedExecutorIdleTimeout
CFG_SPARK_DYNAMIC__ALLOCATION__EXECUTOR__IDLE__TIMEOUT=300s
#spark.dynamicAllocation.enabled
CFG_SPARK_DYNAMIC__ALLOCATION_ENABLED="true"
#spark.dynamicAllocation.initialExecutors
CFG_SPARK_DYNAMIC__ALLOCATION_INITIAL__EXECUTORS=1
#spark.dynamicAllocation.maxExecutors
CFG_SPARK_DYNAMIC__ALLOCATION_MAX__EXECUTORS=5
#spark.dynamicAllocation.minExecutors
CFG_SPARK_DYNAMIC__ALLOCATION_MIN__EXECUTORS=1

#spark.hive.metastore.uris
CFG_SPARK_HIVE_METASTORE_URIS=thrift://metastore:9083

#spark.sql.warehouse.dir
CFG_SPARK_SQL_WAREHOUSE_DIR=file:///warehouse

